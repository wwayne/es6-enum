{
  "name": "es6-enum",
  "version": "2.0.0",
  "description": "Enum type made by es6",
  "main": "dist/enum.js",
  "scripts": {
    "babel": "babel ./src/index.js -o ./build/enum.js",
    "check": "standard ./build/index.js | snazzy",
    "pretest": "npm run build && npm run check",
    "test": "npm run pretest && node test/index.js | tap-spec",
    "prebuild": "rm -rf ./build && rm -rf ./dist",
    "build": "mkdir ./build && npm run babel && webpack",
    "bundle": "cp ./build/* ./dist/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wwayne/es6-enum"
  },
  "keywords": [
    "es6",
    "enum",
    "javascript",
    "data structure"
  ],
  "engines": {
    "node": ">=0.12.0"
  },
  "author": "wwayne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wwayne/es6-enum/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "babel-loader": "^8.1.0",
    "prettier": "^2.0.5",
    "prettier-webpack-plugin": "^1.2.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.4",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
